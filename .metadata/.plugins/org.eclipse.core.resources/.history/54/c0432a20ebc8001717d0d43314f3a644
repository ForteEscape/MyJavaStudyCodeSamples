package com.sehun.problem3;

public class Person {
	public String name;
	public String gender;
	public String age;
	
	Person(){
		
	}
	
	Person(String name, String gender, String age){
		this.name = name;
		this.gender = gender;
		this.age = age;
	}
	
	void setname(String name) {
		this.name = name;
	}
	
	void setgender(String gender) {
		this.gender = gender;
	}
	
	void setage(String age) {
		this.age = age;
	}
}
